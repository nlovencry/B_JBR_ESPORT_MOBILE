<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/card_team">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:padding="6dp"
        android:background="@drawable/bg_list"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp">
        <TextView
            android:id="@+id/tv_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="160dp"
            android:visibility="gone"
            android:textSize="16sp"/>

        <TextView
            android:id="@+id/tv_idt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_id"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="10dp"
            android:text="ID Team"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_idteam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="idteam"
            android:textSize="11sp"
            android:layout_marginLeft="10dp"
            android:layout_below="@id/tv_idt"
            android:maxLines="10"
            />


        <TextView
            android:id="@+id/tv_idc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_idteam"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="10dp"
            android:text="ID Coach"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_idcoach"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Coach"
            android:textSize="11sp"
            android:layout_marginLeft="10dp"
            android:layout_below="@id/tv_idc"
            android:maxLines="10"
            />

        <TextView
            android:id="@+id/tv_nm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_idcoach"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="10dp"
            android:text="Nama Team"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_namat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nama_team"
            android:textSize="11sp"
            android:layout_marginLeft="10dp"
            android:layout_below="@id/tv_nm"
            android:maxLines="10"
            />


    </RelativeLayout>
</RelativeLayout>